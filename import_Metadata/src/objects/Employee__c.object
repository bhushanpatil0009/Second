    <validationRules>
        <fullName>Age_is_between</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(AND(Age__c &gt; 21,Age__c &lt; 30))</errorConditionFormula>
        <errorMessage>“Please provide a valid Date of birth. Employee should not be less than 21 or greater then 30 .”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_Working_Day</fullName>
        <active>true</active>
        <errorConditionFormula>Date_of_Joining__c &gt;  Last_Working_Day__c</errorConditionFormula>
        <errorMessage>Last working day should be greater then joining date please change the resign date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation</fullName>
        <active>true</active>
        <description>If the notice period is true then you must have to enter the resignation date</description>
        <errorConditionFormula>IF(Is_on_Notice_Period__c == FALSE, NOT(ISBLANK(Resignation_Date__c  )), ISBLANK(Resignation_Date__c  ))</errorConditionFormula>
        <errorMessage>Either Unchecked the notice period or enter the resignation date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_IS_10_DIGITS</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( LEN(SSN__c ) =10)</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>SSN must be of 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_is_a_number</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNUMBER(SSN__c))</errorConditionFormula>
